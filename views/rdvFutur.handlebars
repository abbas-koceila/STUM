<div id="title">
<div id="title">
  <h1>Rendez-vous a venir</h1>
</div>

<div class="col-lg-9" id="liste-card">
    <div class="row">
      {{#each rdv}}
        <div class="col-lg-4 col-md-6 mb-4"id={{this.id_rendez_vous}}>
            <div class="card h-100">
              <div class="card-body">
                <h3 class="card-title">Rendez-vous medical</h3>
                <h5>{{this.date_rendez_vous}}</h5>
                <a href="https://www.google.com/maps/place/909+Bd+la+Vérendrye+O,+Gatineau,+QC+J8P+7H2,+Canada/" target="_blank"><h5 class="card-title">Hôpital de Gatineau</h5></a>
           
                <p class="card-text mt-1">Veuillez noter que ceci est juste une version d'essai
                   et que tout rendez-vous est juste une simulation </p>
              </div>
              <div class="card-footer">
                 <div class="participants">
                 </div>
               <button type="button" class="btn btn-danger mb-2 btn-lg btn-block rdvfutur" data-id="{{this.id_urgence}}">Annuler </button>

              </div>
            </div>  
        </div>
      {{/each}}
    </div>
</div>
